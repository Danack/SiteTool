{
    "name": "danack/sitetool",
    "description": "Event based site tool and tester.",
    "keywords": ["crawler"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/amphp/dns"
        }
    ],
    "autoload": {
        "psr-0": {
            "SiteTool": "src/"
        },
        "files": [
          "src/functions.php",
          "src/factories.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "SiteToolTest": "test/"
        }
    },
    "require-dev": {
        "mockery/mockery": "1.0",
        "nikic/fast-route": "0.3.0",
        "phpunit/phpunit": "6.4.4",
        "room11/caching": "^0.0.4",
        "room11/http": "^0.2.0",
        "squizlabs/php_codesniffer": "^2.8",
        "yoast/yoastcs": "0.4.2"
    },
    "require":{
        "php": "^7",
        "rdlowrey/auryn": "^1.4.0",
        "danack/aurynconfig": "^0.9.1",
        "danack/console": "~2.6",
        "danack/tier": "^0.7.1",
        "fluentdom/fluentdom": "~5.3",
        "amphp/artax": "~3.0.14",
        "zendframework/zend-eventmanager": "^3.0",
        "alom/graphviz": "~1.1"
    }
}
